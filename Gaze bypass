-- function cuz lazy
function corner(gui, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius)
    corner.Parent = gui
end
local CAPS = false

function bypass(input)

if input ~= "" then
Notempty = true
else 
Notempty = false
end
  
local NUMBER,IDK,A_1,A_2 ,TextChatService,DefaultChat
NUMBER = { ["0"] = "\xEF\xBC\x90", ["1"] = "\xEF\xBC\x91", ["2"] = "\xEF\xBC\x92", ["3"] = "\xEF\xBC\x93", ["4"] = "\xEF\xBC\x94", ["5"] = "\xEF\xBC\x95", ["6"] = "\xEF\xBC\x96", ["7"] = "\xEF\xBC\x97", ["8"] = "\xEF\xBC\x98", ["9"] = "\xEF\xBC\x99" }
if CAPS then
IDK = input:upper()
else
IDK = input:lower()
end
local result = IDK:gsub("%d", 
function(digit)
return NUMBER[digit] or digit
end)
TextChatService = game:GetService("TextChatService"):FindFirstChild("TextChannels")

if game.PlaceId == 4483381587 then
result = result:gsub("i", "\u{0268}")
result = result:gsub("I", "I\u{030A}")
else
result = result:gsub("i", "\u{0131}\u{0307}")
result = result:gsub("I", "I\u{030A}")
end

result = result:gsub(" ", "\b")
result = result:gsub("|", "\r")

-- Literally The Same Thing
result = result:gsub("a", "\u{0227}")
result = result:gsub("f", "\u{0066}\u{030A}")
result = result:gsub("F", "F\u{030A}")
result = result:gsub("P", "P\u{030A}")
result = result:gsub("p", "·πó")
result = result:gsub("e", "\u{0117}")
result = result:gsub("o", "\u{022F}")
result = result:gsub("u", "\u{1EE5}")

-- sapnu puas
if Notempty then
 A_1 = "\u{A671}>\b" .. result
 A_2 = "All"
    if TextChatService then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(A_1)
    else
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer( A_1, A_2)
    end
end
end
    

    
    
    


